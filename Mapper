            <script src="js/gjs.js"></script> <script type="text/javascript">
             	function initMap(){
             		// Map options
             var option = {
             	zoom:9,
             	center:{lat:49.894066,lng:2.295753}
             	}
             	// New Map
             	var map = new
             	google.maps.Map(document.getElementById('map'),option);
             	// listen forclick
             	google.maps.event.addListener(map, 'click',
             function(event){
             	addMarker({coords:event.latLng});
             });




             /*

             	// Add marker
             	var marker = new google.maps.Marker({
             position:{lat:48.864716,lng:2.349014},
             map:map,
             icon:'https://developers.google.com/maps/documentation/javascript/examples/full/images/beachflag.png'
             });

             var infoWindow = new google.maps.InfoWindow({
             	content:'<h1>Test</h1>'
             });

             marker.addListener('click',function(){
             infoWindow.open(map, marker);
             });
             }*/

             // array of Markers
             var markers = [
             {
             	coords:{lat:48.864716,lng:2.349014},
             iconImage:'https://developers.google.com/maps/documentation/javascript/examples/full/images/beachflag.png',
             content:'<h1>test1</h1>'

             },
             {
             	coords:{lat:49.894066,lng:2.295753},
             iconImage:'https://developers.google.com/maps/documentation/javascript/examples/full/images/beachflag.png',
             content:'<h1>test2</h1>'

             }


             ];

             // loop incrémentation
             for (var i = 0;i < markers.length; i++){
             	addMarker(markers[i]);
             }



             // add Marker function
             function addMarker(props){
             	var marker = new google.maps.Marker({
             position:props.coords,

             map:map,
             //icon:'props.iconImage'
             });
             	// verif si ya une icone custom
             	if(props.iconImage){
             		// set icon image
             		marker.setIcon(props.iconImage);
             		}
             	if (props.content) {
             var infoWindow = new google.maps.InfoWindow({
             	content:props.content
             });

             marker.addListener('click',function(){
             infoWindow.open(map, marker);
             });
             	}
             	}
             }
             </script>
                  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDjjCnY1j15VAmLK3PbpncuyFgVAa9RqTM&callback=initMap"
                 async defer></script>
